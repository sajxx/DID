{
  "name": "connection-org1",
  "version": "1.0.0",
  "client": {
    "organization": "Org1",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },
  "channels": {
    "did-channel": {
      "orderers": [
        "orderer.did.com"
      ],
      "peers": {
        "peer0.org1.did.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer0.org2.did.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        }
      }
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "peer0.org1.did.com"
      ],
      "certificateAuthorities": [
        "ca.org1.did.com"
      ]
    },
    "Org2": {
      "mspid": "Org2MSP",
      "peers": [
        "peer0.org2.did.com"
      ],
      "certificateAuthorities": [
        "ca.org2.did.com"
      ]
    }
  },
  "orderers": {
    "orderer.did.com": {
      "url": "grpcs://localhost:7050",
      "tlsCACerts": {
        "path": "crypto-config/ordererOrganizations/did.com/orderers/orderer.did.com/msp/tlscacerts/tlsca.did.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "orderer.did.com",
        "hostnameOverride": "orderer.did.com"
      }
    }
  },
  "peers": {
    "peer0.org1.did.com": {
      "url": "grpcs://localhost:7051",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/org1.did.com/peers/peer0.org1.did.com/msp/tlscacerts/tlsca.org1.did.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org1.did.com",
        "hostnameOverride": "peer0.org1.did.com"
      }
    },
    "peer0.org2.did.com": {
      "url": "grpcs://localhost:9051",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/org2.did.com/peers/peer0.org2.did.com/msp/tlscacerts/tlsca.org2.did.com-cert.pem"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org2.did.com",
        "hostnameOverride": "peer0.org2.did.com"
      }
    }
  },
  "certificateAuthorities": {
    "ca.org1.did.com": {
      "url": "https://localhost:7054",
      "caName": "ca-org1",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/org1.did.com/ca/ca.org1.did.com-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    },
    "ca.org2.did.com": {
      "url": "https://localhost:8054",
      "caName": "ca-org2",
      "tlsCACerts": {
        "path": "crypto-config/peerOrganizations/org2.did.com/ca/ca.org2.did.com-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    }
  }
}
